                                             <image>
                                                <Image url="@../images/Zrzut%20ekranu%202024-01-15%20175832.png" />
                                             </image>
                                          </ImageView>
                                       </children>
                                    </AnchorPane>
                              </content>
                            </Tab>
                          </tabs>
                        </TabPane>
